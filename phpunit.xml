<?xml version="1.0" encoding="UTF-8"?>
<phpunit>
    <testsuites>
        <testsuite name="small">
	    <directory>tests</directory>
	    <exclude>tests/src/BaseClassTest.php</exclude>
        </testsuite>
    </testsuites>

    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
	    <directory suffix=".php">src</directory>
        </whitelist>
    </filter>
    <logging>
	<log type="coverage-html" target="logs" lowUpperBound="35"
            highLowerBound="70"/>
    </logging>
</phpunit>
